<!DOCTYPE html>
<html>
<head>
  <!--
      For demonstration purposes, the `title` is dynamically set here based on
      your `sails.config.appName` to show that you can inject data into layouts
      exactly the same way as with your other view templates.
  -->
  <title><%- title %></title>
  <!-- Viewport mobile tag for sensible mobile support -->
  <meta name="viewport" content="width=device-width, initial-scale=1,
  maximum-scale=1"/>

  <!--[if lt IE 9]>
  <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
  <!--STYLES-->
  <link rel="stylesheet" href="/styles/ie_phuc.css">
  <link rel="stylesheet" href="/styles/jquery.contextMenu.css">
  <link rel="stylesheet" href="/styles/main.css">
  <link rel="stylesheet" href="/styles/nanoscroller.css">
  <link rel="stylesheet" href="/styles/style_phuc.css">
  <link rel="stylesheet" href="/styles/tooltipster.css">
  <!--STYLES END-->
</head>
<body>
<div class="container">
  <!--Header-->
  <header class="header">
    <div class="top"></div>
    <div class="container_1080 border-bottom ">
      <ul class="header-content">
        <li class="header-content-cell">
          <ul>
            <li>
              <div class="heading heading-left">
                <span id="btnDebate">JOIN FANCHILADA</span>
              </div>
            </li>
            <li>
              <ul class="list-icon">
                <li><a href=""><span class="iconsp-3504-35px"></span></a></li>
                <li><a href=""><span class="iconsp-3505-35px"></span></a></li>
                <li><a href=""><span class="iconsp-3506-35px"></span></a></li>
                <li><a href=""><span class="iconsp-3507-35px"></span></a></li>
                <li><a href=""><span class="iconsp-3508-35px"></span></a></li>
              </ul>
            </li>
            <li>
              <div class="info">
                <span class="bolder">NOW</span><span>World Cup Qualifier, Mexico Vs USA</span>
              </div>
            </li>
            <li>
              <div class="time">
                <span class="bold">time remaining 45:03</span>
                <span class="iconsp-line"></span>
                <span class="bolder">join debate</span>
              </div>
            </li>
          </ul>
        </li>
        <li class="header-logo">
          <figure><img src="../images/logo.png" height="196"></figure>
        </li>
        <li class="header-content-cell right-cell">
          <ul>
            <li>
              <div class="heading heading-right" id="background-right">

                <span id="btnLogoutDebate">LOG-IN</span>
              </div>
            </li>
            <li>
              <ul class="list-icon">
                <li><a href=""><span class="iconsp-3501-35px"></span></a></li>
                <li><a href=""><span class="iconsp-bycycle"></span></a></li>
                <li><a href=""><span class="iconsp-3502-35px"></span></a></li>
                <li><a href=""><span class="iconsp-double-ball"></span></a></li>
                <li><a href=""><span class="iconsp-3503-35px"></span></a></li>
              </ul>
            </li>
            <li>
              <div class="info">
                <span class="bolder">NEXT</span><span>Reaction to NY Yankees vs Bulls</span>
              </div>
            </li>
            <li>
              <div class="time">
                <span class="bold">Starts at 12:00</span>
                <span class="iconsp-line"></span>
                <span class="bolder">join debate</span>
              </div>
            </li>
          </ul>
        </li>
      </ul>
    </div>
  </header>
  <div class="clear"></div>
  <!-- Include the view file for the current controller/route -->
  <div class="container_1080">
    <%- body %>
  </div>
  <footer class="main-footer">
    <div class="container_1080 clearfix">
      <nav>
        <ul class="navigation">
          <li><a href="#">about fanchilada</a></li>
          <li><a href="#">search</a></li>
          <li><a href="#">contact us</a></li>
          <li><a href="#">faqs</a></li>
        </ul>
        <ul class="privacy">
          <li>Terms of use</li>
          <li>Privacy Policy</li>
          <li>Data Protection</li>
        </ul>
        <div class="clearfix"></div>
      </nav>
    </div>
  </footer>
</div>
<!--SCRIPTS-->
<script src="/js/jquery-1.10.2.min.js"></script>
<script src="/js/jquery.contextMenu.js"></script>
<script src="/js/jquery.nanoscroller.min.js"></script>
<script src="/js/jquery.tooltipster.js"></script>
<script src="/js/jquery.ui.position.js"></script>
<script src="/js/socket.io.js"></script>
<script src="/js/sails.io.js"></script>
<script src="/js/app.js"></script>
<script src="/js/underscore-min.js"></script>
<script src="/js/backbone-min.js"></script>
<script src="/js/chat-dsv.js"></script>
<script src="/js/swfobject.js"></script>
<!--SCRIPTS END-->
</body>
</html>
